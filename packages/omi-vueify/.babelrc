{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "jsxPragma": "h",
        "jsxPragmaFrag": "h.f"
      }
    ],
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "h.f"
      }
    ],
    ["@babel/plugin-proposal-decorators", { "version": "2023-05" }],
    "@babel/plugin-proposal-class-properties"
  ]
} 