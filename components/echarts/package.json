{
  "name": "@omiu/echarts",
  "description": "OMI version of ECharts",
  "version": "0.0.1",
  "main": "dist/index.es.js",
  "exports": {
    ".": "./dist/index.es.js"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "docsExtend": {
    "cnName": "ECharts 的 OMI 版本",
    "cnDescription": "ECharts 的 OMI 版本"
  },
  "scripts": {
