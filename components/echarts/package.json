  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.33",
    "@types/node": "^16.4.7",
    "@web/test-runner": "^0.13.15",
    "omi": "^6.19.23",
    "sass": "^1.36.0",
    "typescript": "^4.3.2",
    "vite": "^2.4.3"
  },
  "license": "MIT",
  "keywords": [
    "omi",
    "omiu",
    "tag"
